{
  "name": "server",
  "version": "1.0.0",
  "description": "A server app that will be capable of serving media files from anywhere, wrapped in an electron app and built using React",
  "main": "main.js",
  "productName": "cloudMe",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "watch": "webpack -d --progress --watch",
    "rebuild": "webpack -d --progress --colors",
    "bundle": "webpack -p --progress --colors",
    "package": "electron-packager . cloudMe --platform=darwin --arch=x64"
  },
  "keywords": [
    "server",
    "electron",
    "media",
    "server"
  ],
  "author": "Joey Pinhas",
  "license": "MIT",
  "engines": {
