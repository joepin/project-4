    "electron-settings": "^2.2.2",
    "express": "^4.14.0",
    "fs": "0.0.1-security",
    "fs-extra": "^1.0.0",
    "getmac": "^1.2.1",
    "mime": "^1.3.4",
    "morgan": "^1.7.0",
    "ngrok": "^2.2.4",
    "node-fetch": "^1.6.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
